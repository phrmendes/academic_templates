  file = {C\:\\Users\\PEDROHENRIQUEROCHAME\\Zotero\\storage\\3N5THMZS\\Cano - 2017 - Brasil – construção e desconstrução do desenvolvim.pdf;C\:\\Users\\PEDROHENRIQUEROCHAME\\Zotero\\storage\\3S72MXZX\\8650933.html},
