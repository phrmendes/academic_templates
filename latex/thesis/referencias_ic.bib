@phdthesis{saesComercioEcologicamenteDesigual2017,
  title = {Com\'ercio Ecologicamente Desigual No S\'eculo {{XXI}} : Evid\^encias a Partir Da Inser\c{c}\~ao Brasileira No Mercado Internacional de Min\'erio de Ferro},
  shorttitle = {Com\'ercio Ecologicamente Desigual No S\'eculo {{XXI}}},
  author = {Saes, Beatriz Macchione},
  year = {2017},
  address = {{Campinas}},
  abstract = {Resumo: O presente trabalho analisa as especificidades do com\'ercio ecologicamente desigual no s\'eculo XXI a partir da observa\c{c}\~ao do metabolismo social do min\'erio de ferro e da inser\c{c}\~ao brasileira neste mercado. A hip\'otese levantada \'e que existe um com\'ercio ecologicamente desigual Norte-Sul, que se reflete em uma transfer\^encia material assim\'etrica aos pa\'ises do Norte, cuja din\^amica \'e importante para compreender as caracter\'isticas do recente boom das commodities e as implica\c{c}\~oes sobre os pa\'ises exportadores de produtos prim\'arios. Primeiramente, mostramos como as evid\^encias de desmaterializa\c{c}\~ao das economias desenvolvidas s\~ao superestimadas pelas an\'alises que desconsideram a crescente fragmenta\c{c}\~ao internacional da produ\c{c}\~ao nas \'ultimas d\'ecadas. Na medida em que as economias avan\c{c}adas especializam-se em etapas localizadas nas duas pontas das cadeias de produ\c{c}\~ao, baseadas em ativos intang\'iveis (pesquisa, desenho, marketing etc.), o peso material de suas economias se reduz em rela\c{c}\~ao ao PIB, mas a captura da maior parcela do valor adicionado das cadeias mant\'em padr\~oes de consumo elevados baseados em estoques materiais "ocultos" em outros pa\'ises. Em segundo lugar, discutimos como a crescente transnacionaliza\c{c}\~ao do espa\c{c}o asi\'atico, decorrente desta fragmenta\c{c}\~ao internacional da produ\c{c}\~ao, promoveu uma amplia\c{c}\~ao sem precedentes da demanda por recursos naturais, alimentando o ciclo recente de valoriza\c{c}\~ao dos pre\c{c}os das commodities. No setor de min\'erio de ferro, a despeito de sua moderniza\c{c}\~ao e de maiores possibilidades de capta\c{c}\~ao de financiamento, a rigidez da oferta amplificou o aumento dos pre\c{c}os, possibilitando a entrada de produtores de alto custo voltados \`a extra\c{c}\~ao de min\'erio de ferro de baixa qualidade. No Brasil, isso significou a expans\~ao da fronteira miner\'aria de ferro principalmente em dire\c{c}\~ao ao norte de Minas Gerais e \`a Bahia, em regi\~oes de reconhecida import\^ancia ambiental ou habitadas por pequenos agricultores, comunidades quilombolas e ribeirinhas. A din\^amica perversa por tr\'as do boom de pre\c{c}os seria evidenciada com a revers\~ao destes. A despeito das interven\c{c}\~oes irrevers\'iveis no meio ambiente e o comprometimento do modo de vida de comunidades inteiras, a viabilidade financeira dos empreendimentos de alto custo na fronteira miner\'aria viu-se comprometida. A decorrente desvaloriza\c{c}\~ao dos ativos de tais empreendimentos, com frequ\^encia adquiridos por outras empresas e reconfigurados para operar mesmo com uma rentabilidade muito baixa, agravaria ainda mais o excesso de oferta no setor, pressionando negativamente os pre\c{c}os e amplificando a transfer\^encia de recursos materiais ao exterior. Por fim, descrevemos como as estrat\'egias de desenvolvimento adotadas no Brasil durante o boom das commodities refletiram-se no setor miner\'ario, contribuindo para impulsionar um modelo "neoextrativista" no pa\'is. Em particular, discutimos a regulamenta\c{c}\~ao do setor e o modelo de desenvolvimento presente na proposta do novo c\'odigo da minera\c{c}\~ao, assim como, os limites do nacionalismo dos recursos naturais como resposta \`a inser\c{c}\~ao internacional subordinada do pa\'is. O modelo "neoextrativista" foi evidenciado pelas disputas em torno da legisla\c{c}\~ao ambiental brasileira, em que prevaleceu o "interesse nacional" como defendido pelo setor miner\'ario, a despeito de outros interesses e valores concorrentes \`a minera\c{c}\~ao. Assim, a aus\^encia de vis\~oes e modelos de desenvolvimento alternativos contribuiu para refor\c{c}ar o car\'ater estrutural do com\'ercio ecologicamente desigual Norte-Sul},
  file = {C\:\\Users\\PEDROHENRIQUEROCHAME\\Zotero\\storage\\86PQH5NT\\Saes - 2017 - Comércio ecologicamente desigual no século XXI  e.pdf;C\:\\Users\\PEDROHENRIQUEROCHAME\\Zotero\\storage\\Y3GCM72M\\321931.html},
  school = {IE-UNICAMP}
}


